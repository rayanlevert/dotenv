# .env file sample

# production | development | staging | testing
APP_ENV=development
APP_NAME="Name of the application"
APP_URL=https://test.local.com
APP_CACHE=/app/cache
APP_LOGS=/app/logs

# Application
WEB_DOCUMENT_ROOT=/app/public
WEB_ALIAS_DOMAIN=test.local.com
WEB_DOCUMENT_ASSETS=${WEB_DOCUMENT_ROOT}/assets

# MySQL settings
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=test_base
MYSQL_USER=root
MYSQL_PASSWORD=root
MYSQL_PORT=33061
MYSQL_HOST=localhost
MYSQL_DSN=mysql:host=${MYSQL_HOST};dbname=${MYSQL_DATABASE};port=${MYSQL_PORT}

# REDIS settings
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_INDEX=0
REDIS_LIFETIME=7200
REDIS_PERSISTENT=true

# Session settings
SESSION_PERSISTENT=true
SESSION_LIFETIME=43200
SESSION_SECURE=1
SESSION_HTTPONLY=1

