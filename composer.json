{
    "name": "rayanlevert/dotenv",
    "description": "Dependency-free environment file handler (.env) loading variables and its values in $_ENV, $_SERVER and getenv() in the PHP userland",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "php",
        "env",
        "dotenv"
    ],
    "support": {
        "issues": "https://github.com/rayanlevert/dotenv/issues"
    },
    "authors": [
        {
            "name": "Rayan Levert",
            "email": "rayanlevert@msn.com"
        }
    ],
    "require": {
        "php": ">=8.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.9",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.4.1"
        }
    },
    "autoload": {
        "psr-4": {
            "RayanLevert\\Dotenv\\": "src/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "RayanLevert\\Dotenv\\Tests\\": "tests/"
        }
    }
}